<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Style TargetType="Button" x:Key="FaBrandRegularButton">
        <Setter Property="FontFamily" Value="FontAwesomeBrandsRegular" />
        <Setter Property="FontSize" Value="20" />
    </Style>

    <Style TargetType="Label" x:Key="FaBrandRegularLabel">
        <Setter Property="FontFamily" Value="FontAwesomeBrandsRegular" />
    </Style>

    <Style TargetType="Button" Class="FaBrandRegularButton">
        <Setter Property="FontFamily" Value="FontAwesomeBrandsRegular" />
        <Setter Property="FontSize" Value="20" />
    </Style>

    <Style TargetType="Label" Class="FaBrandRegularLabel">
        <Setter Property="FontFamily" Value="FontAwesomeBrandsRegular" />
    </Style>

</ResourceDictionary>