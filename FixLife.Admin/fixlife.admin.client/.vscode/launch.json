    {
      "type": "chrome",
      "request": "launch",
      "name": "localhost (Chrome)",
      "url": "https://localhost:${env:DEV_SERVER_PORT}",
      "webRoot": "${workspaceFolder}",
      "preLaunchTask": "npm: start"
    }
  ]
}
